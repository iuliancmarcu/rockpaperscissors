{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "array-type": [
      true,
      "generic"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-alignment": [
      false
    ],
    "jsx-no-multiline-js": [
      false
    ],
    "jsx-wrap-multiline": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "max-line-length": [
      true,
      160
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "no-implicit-dependencies": [
      false
    ],
    "no-submodule-imports": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "constructor": "never",
        "method": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
